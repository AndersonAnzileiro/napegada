@{
    ViewBag.Title = "Home";
}



    @Html.Partial("_Menu")

    


<div class="container">

    <h2>HOME</h2>
    @if (Request.Cookies["napegada_auth"] != null)
    {
        @FormsAuthentication.Decrypt(Request.Cookies["napegada_auth"].Value).Name
    }
    else
    {
        <p>Nao tem cookie !!!</p>
    }

</div>

@section scripts{
    @Scripts.Render("~/Scripts/jquery.validate.min.js",
                            "~/Scripts/jquery.validate.unobtrusive.min.js",
                                    "~/Scripts/jquery.unobtrusive-ajax.min.js")
}